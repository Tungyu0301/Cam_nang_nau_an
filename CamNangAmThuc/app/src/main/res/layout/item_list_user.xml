<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:paddingTop="@dimen/_5sdp"
            android:paddingStart="@dimen/_8sdp"
            android:paddingEnd="@dimen/_8sdp"
            android:paddingBottom="@dimen/_7sdp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:layout_alignParentEnd="true"
                android:id="@+id/iconOptiondeleteuse"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                app:srcCompat="@drawable/ic_delete_use"
                tools:ignore="VectorDrawableCompat" />
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_below="@id/iconOptiondeleteuse"
                android:id="@+id/imgAvatarusead"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"
                app:srcCompat="@drawable/ic_avatar_user"
                tools:ignore="VectorDrawableCompat" />
            <TextView
                android:layout_below="@id/imgAvatarusead"
                android:id="@+id/txtNameusead"
                android:layout_marginTop="@dimen/_3sdp"
                android:layout_marginBottom="@dimen/_15sdp"
                android:textStyle="bold"
                android:textSize="@dimen/_16sdp"
                android:layout_width="wrap_content"
                android:textColor="@color/colorTextName"
                android:layout_height="wrap_content" />
            <ImageView
                android:layout_below="@id/txtNameusead"
                android:id="@+id/iconPhone"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_width="@dimen/_13sdp"
                android:layout_height="@dimen/_13sdp"
                app:srcCompat="@drawable/ic_phone_android_black_24dp"
                tools:ignore="VectorDrawableCompat" />
            <TextView
                android:id="@+id/txtPhoneusead"
                android:textSize="@dimen/_13sdp"
                android:layout_below="@id/txtNameusead"
                android:layout_toEndOf="@id/iconPhone"
                android:textColor="@color/colorTextMedium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <View
                android:id="@+id/viewPhoneuse"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:layout_below="@id/iconPhone"
                android:layout_width="match_parent"
                android:background="@color/colorView"
                android:layout_height="1dp"/>

            <ImageView
                android:layout_below="@id/viewPhoneuse"
                android:id="@+id/iconEmail"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_width="@dimen/_13sdp"
                android:layout_height="@dimen/_13sdp"
                app:srcCompat="@drawable/ic_email_black_24dp"
                tools:ignore="VectorDrawableCompat" />
            <TextView
                android:id="@+id/txtEmailusead"
                android:textSize="@dimen/_13sdp"
                android:layout_below="@id/viewPhoneuse"
                android:layout_toEndOf="@id/iconEmail"
                android:textColor="@color/colorTextMedium"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <View
                android:id="@+id/viewEmail"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:layout_below="@id/iconEmail"
                android:layout_width="match_parent"
                android:background="@color/colorView"
                android:layout_height="1dp"/>

            <ImageView
                android:layout_below="@id/viewEmail"
                android:id="@+id/ngaysinhuse"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_width="@dimen/_13sdp"
                android:layout_height="@dimen/_13sdp"
                app:srcCompat="@drawable/ic_birthday_back_24dp"
                tools:ignore="VectorDrawableCompat" />
            <TextView
                android:id="@+id/txtngaysinhusead"
                android:textSize="@dimen/_13sdp"
                android:layout_below="@id/viewEmail"
                android:layout_toEndOf="@id/ngaysinhuse"
                android:textColor="@color/colorInputFacebook"
                android:inputType="textPassword"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:id="@+id/viewmk"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:layout_below="@id/ngaysinhuse"
                android:layout_width="match_parent"
                android:background="@color/colorView"
                android:layout_height="1dp"/>
            <ImageView
                android:layout_below="@id/viewmk"
                android:id="@+id/diachiuse"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_width="@dimen/_13sdp"
                android:layout_height="@dimen/_13sdp"
                app:srcCompat="@drawable/ic_maps_back_24dp"
                tools:ignore="VectorDrawableCompat" />
            <TextView
                android:id="@+id/txtdiachiusead"
                android:textSize="@dimen/_13sdp"
                android:layout_below="@id/viewmk"
                android:layout_toEndOf="@id/diachiuse"
                android:textColor="@color/colorInputFacebook"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <View
                android:id="@+id/viewdiachi"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:layout_below="@id/diachiuse"
                android:layout_width="match_parent"
                android:background="@color/colorView"
                android:layout_height="1dp"/>
            <ImageView
                android:layout_below="@id/viewdiachi"
                android:id="@+id/iconcode"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_width="@dimen/_13sdp"
                android:layout_height="@dimen/_13sdp"
                app:srcCompat="@drawable/ic_soucode"
                tools:ignore="VectorDrawableCompat" />
            <TextView
                android:id="@+id/txtcodeusead"
                android:textSize="@dimen/_13sdp"
                android:layout_below="@id/viewdiachi"
                android:layout_toEndOf="@id/iconcode"
                android:textColor="@color/colorInputFacebook"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

</LinearLayout>